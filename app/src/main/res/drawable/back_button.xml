<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="50dp"
                android:height="50dp"/>
        </shape>
    </item>

    <item
        android:width="18dp"
        android:height="3dp"
        android:top="15dp"
        android:right="14dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="25">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="18dp"
        android:height="3dp"
        android:top="-7dp"
        android:right="14dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="-25">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="30dp"
        android:height="3dp"

        android:top="4dp"
        android:gravity="center"
        >
        <rotate
            android:fromDegrees="0">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="60dp"
        android:height="60dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

</layer-list>